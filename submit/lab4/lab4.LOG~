    [38;5;28mint[39;49m [38;5;130mc[39;49m = getchar();[K
    [38;5;129mwhile[39;49m (c != [38;5;89m'\n'[39;49m && c != EOF) { [38;5;124m//waste line                                                                                        [39;49m
