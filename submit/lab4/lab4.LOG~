    }[K
    [38;5;129melse[39;49m {[K
      [38;5;28mbool[39;49m [38;5;130misOk[39;49m = (p == &ints[index]);[K
      [38;5;28mint[39;49m [38;5;130mval[39;49m [38;5;129m__attribute__[39;49m((unused)) = *p;[K
      printf([38;5;89m"%s\n"[39;49m, isOk ? [38;5;89m"ok"[39;49m : [38;5;89m"not ok"[39;49m);[K
      [38;5;129mif[39;49m (isOk) {[K
        nOk++;[K
        nRun++;[K
      }[K
      [38;5;129melse[39;49m {[K
        nErr++;[K
        nRun = 0;[K
      }[K
